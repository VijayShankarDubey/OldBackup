
public class Chapter_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int score = 72;
		char grade;
		
		if(score >= 80) {
			grade = 'A';
		}
			else if(79  >= 70) {
				grade ='B';
			} else if(69 >= 60) {
				grade ='C';
			} else if(59 >= 50) {
				grade ='D';
			} else {
				grade= 'E';
			}
		System.out.println("Grade : " + grade);
		}
}


