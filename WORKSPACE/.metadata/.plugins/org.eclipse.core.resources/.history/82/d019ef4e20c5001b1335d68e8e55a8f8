package javaTutorial;

public class setThree {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int count = 1;
		for (int i = 1; i < 4; i++) {

			for (int j = 1; j < i * count; j++) {
				System.out.print(j + "  ");
				count++;
			}
			System.out.println(" ");
		}

	}

}
